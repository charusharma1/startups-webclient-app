
<%=form_tag "/startups", method: :post do%>


  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= label_tag :location %>
    <%= text_field_tag :location %>
  </div>
  <div>
    <%= label_tag :job_openings %>
    <%= text_field_tag :job_openings %>
  </div>
  <div>
    <%= submit_tag 'Create Startup!' %>
  </div>

<% end %>